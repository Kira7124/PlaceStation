<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.project3.placestation.repository.interfaces.BizRepository">
	
	
	
	<!-- 관리자사업자명단출력 -->
	
	<select id="BizAll" resultType="com.project3.placestation.repository.entity.Biz">
		select * from biz
		limit #{startPage},#{pageSize}
	</select>
	
	<!-- 페이징처리 갯수셋팅 -->
	<select id = "countBiz" resultType="int">
		<![CDATA[ select count(*) from biz where biz_no > 0 ]]>
	</select>

	





</mapper>